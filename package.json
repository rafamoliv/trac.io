{
  "name": "trac.io",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.19.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
